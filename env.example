# Jenkins Configuration
JENKINS_PORT=8080
JENKINS_AGENT_PORT=50000

# Java Options
# Adjust heap size based on available RAM
# -Xms: Initial heap size
# -Xmx: Maximum heap size
JENKINS_OPTS=-Xms512m -Xmx2048m -Djenkins.install.runSetupWizard=true

# Timezone (use your local timezone)
TZ=UTC

# Jenkins URL (update after setting up domain)
JENKINS_URL=http://localhost:8080
